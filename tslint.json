{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [false],
        "no-string-literal": [false],
        "max-line-length": [true, {
            "limit": 200,
            "ignore-pattern": "^import [^,]+ from |^export | implements"
        }]
    },
    "rulesDirectory": []
}